# File generated by Chef, do not edit manually

<% if @config_dir %>
config-dir= <%= @config_dir %>
<% end %>
<% if @socket_dir %>
socket-dir=<%= @socket_dir %>
<% end %>
<% if @setgid %>
setgid=<%= @setgid %>
<% end %>
<% if @setuid %>
setuid=<%= @setuid %>
<% end %>
<% if @launch %>
launch=<%= @launch.join(',') %>
<% end %>

<% @variables.sort.each do |key, value| %>
<% next if value.nil? %>
<%= key.to_s.gsub('_', '-') %>=<%=
  case value
  when TrueClass
    'yes'
  when FalseClass
    'no'
  when Array
    value.join(',')
  else
    value
  end
%>
<% end %>
